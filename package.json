{
  "name": "documentation_robotics_viewer",
  "version": "0.2.0",
  "private": true,
  "description": "Basic graph presentation of the documentation robotics meta-model",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "vite --config vite.config.embedded.ts --port 3001 --host",
    "build": "vite build --config vite.config.embedded.ts && npm run package:embedded",
    "package:embedded": "node scripts/package-embedded.cjs",
    "preview": "vite preview --config vite.config.embedded.ts",
    "server": "node server/index.cjs",
    "start": "bash scripts/start-servers.sh",
    "verify": "bash scripts/verify-setup.sh",
    "test": "playwright test",
    "test:debug": "playwright test --debug",
    "test:e2e": "playwright test --config=playwright.e2e.config.ts",
    "test:e2e:ui": "playwright test --config=playwright.e2e.config.ts --ui",
    "test:e2e:headed": "playwright test --config=playwright.e2e.config.ts --headed",
    "refine:motivation": "playwright test tests/refinement/motivation-refinement.spec.ts --config=playwright.refinement.config.ts",
    "refine:business": "playwright test tests/refinement/business-refinement.spec.ts --config=playwright.refinement.config.ts",
    "refine:c4": "playwright test tests/refinement/c4-refinement.spec.ts --config=playwright.refinement.config.ts",
    "refine:interactive": "playwright test tests/refinement/interactive-refinement.spec.ts --config=playwright.refinement.config.ts --headed",
    "refine:all": "playwright test tests/refinement/*.spec.ts --config=playwright.refinement.config.ts",
    "metrics:report": "playwright test tests/metrics/metrics-report.spec.ts --config=playwright.refinement.config.ts",
    "metrics:regression-check": "playwright test tests/metrics/regression-check.spec.ts --config=playwright.refinement.config.ts",
    "metrics:update-baselines": "UPDATE_BASELINES=true playwright test tests/metrics/regression-check.spec.ts --config=playwright.refinement.config.ts -g 'should update baselines'",
    "metrics:all": "playwright test tests/metrics/*.spec.ts --config=playwright.refinement.config.ts",
    "report:refinement": "playwright show-report playwright-report/refinement --host 0.0.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@tanstack/react-router": "^1.139.12",
    "@types/d3-force": "^3.0.10",
    "@types/js-yaml": "^4.0.9",
    "@types/uuid": "^10.0.0",
    "@xyflow/react": "^12.9.3",
    "cors": "^2.8.5",
    "d3-force": "^3.0.0",
    "dagre": "^0.8.5",
    "express": "^5.1.0",
    "flowbite-react": "^0.12.13",
    "html-to-image": "^1.11.13",
    "imghash": "^1.1.2",
    "js-yaml": "^4.1.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "sharp": "^0.34.5",
    "ssim.js": "^3.5.0",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/dagre": "^0.7.53",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "axe-playwright": "^2.2.2",
    "flowbite": "^4.0.1",
    "playwright": "^1.57.0",
    "recast": "^0.23.11",
    "tailwindcss": "^4.1.17",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.4"
  }
}
